<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Profile" android:background="#F3C7D3">

    <TextView
            android:text="K No"
            android:layout_width="100dp"
            android:layout_height="38dp"
            android:id="@+id/textView9" android:layout_marginStart="40dp"

            app:layout_constraintStart_toStartOf="parent"
            android:textColor="#161212" android:textSize="28sp"
            android:fontFamily="casual" android:layout_marginTop="40dp"
            app:layout_constraintTop_toBottomOf="@+id/textView8"/>
    <TextView
            android:text="Name"
            android:layout_width="100dp"
            android:layout_height="38dp"
            android:id="@+id/textView6" android:layout_marginStart="40dp"

            app:layout_constraintStart_toStartOf="parent"
            android:textColor="#161212" android:textSize="28sp"
            android:fontFamily="casual" android:textAlignment="center" android:layout_marginTop="72dp"
            app:layout_constraintTop_toTopOf="parent"/>
    <TextView
            android:text="Address"
            android:layout_width="120dp"
            android:layout_height="50dp"
            android:id="@+id/textView7" android:layout_marginStart="40dp"

            app:layout_constraintStart_toStartOf="parent"
            android:textColor="#161212" android:textSize="28sp"
            android:fontFamily="casual" android:layout_marginTop="108dp"
            app:layout_constraintTop_toBottomOf="@+id/textView8"/>
    <TextView
            android:text="Phone no"
            android:layout_width="125dp"
            android:layout_height="39dp"
            android:id="@+id/textView8" android:layout_marginStart="40dp"

            app:layout_constraintStart_toStartOf="parent"
            android:textColor="#161212" android:textSize="28sp"
            android:fontFamily="casual" android:layout_marginTop="48dp"
            app:layout_constraintTop_toBottomOf="@+id/textView6"/>
    <TextView
            android:layout_width="180dp"
            android:layout_height="41dp"
            android:id="@+id/name"
            android:layout_marginTop="72dp"
            app:layout_constraintTop_toTopOf="parent" android:fontFamily="monospace"
            tools:layout_editor_absoluteX="296dp"/>
    <TextView
            android:layout_width="180dp"
            android:layout_height="41dp"
            android:id="@+id/name2"
            android:layout_marginTop="72dp"
            app:layout_constraintTop_toTopOf="parent" android:fontFamily="monospace"
            app:layout_constraintStart_toEndOf="@+id/textView6" android:layout_marginStart="64dp" android:text="Kamad"
            android:textColor="#0E0D0D" android:textSize="28sp" android:textStyle="normal"/>
    <TextView
            android:layout_width="180dp"
            android:layout_height="41dp"
            android:id="@+id/name3" app:layout_constraintStart_toEndOf="@+id/textView9"
            android:layout_marginStart="64dp" android:layout_marginTop="232dp"
            app:layout_constraintTop_toTopOf="parent" android:fontFamily="monospace"/>
    <TextView
            android:layout_width="173dp"
            android:layout_height="50dp"
            android:id="@+id/name5"
            android:layout_marginStart="208dp" android:layout_marginTop="388dp"
            app:layout_constraintTop_toTopOf="parent" android:fontFamily="monospace"
            app:layout_constraintStart_toStartOf="parent" android:textSize="24sp" android:text="2 Lakh"/>
    <TextView
            android:layout_width="183dp"
            android:layout_height="39dp"
            android:id="@+id/name4" app:layout_constraintStart_toEndOf="@+id/textView8"
            android:layout_marginStart="40dp" android:layout_marginTop="44dp"
            android:fontFamily="monospace" app:layout_constraintTop_toBottomOf="@+id/name2" android:textSize="24sp"
            android:textColor="#111010" android:text="1234567890"/>
    <TextView
            android:text="192-B Malviya nagar,Jaipur,Rajasthan"
            android:layout_width="210dp"
            android:layout_height="50dp"
            android:id="@+id/textView10" app:layout_constraintStart_toEndOf="@+id/textView7"
            android:layout_marginStart="44dp" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/name3" android:textSize="18sp" android:textColor="#131112"/>
    <TextView
            android:text="Salary"
            android:layout_width="166dp"
            android:layout_height="52dp"
            android:id="@+id/textView26" android:textColor="#131011"
            android:layout_marginTop="36dp" app:layout_constraintTop_toBottomOf="@+id/textView7"
            android:textSize="30sp" android:layout_marginStart="36dp" app:layout_constraintStart_toStartOf="parent"/>
</android.support.constraint.ConstraintLayout>